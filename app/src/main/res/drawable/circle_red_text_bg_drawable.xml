<?xml version="1.0" encoding="utf-8"?><!--<shape xmlns:android="http://schemas.android.com/apk/res/android">-->

<!--&lt;!&ndash; 实心 &ndash;&gt;--><!--<solid android:color="@color/colorOrigin" />--><!--&lt;!&ndash; 圆角 &ndash;&gt;--><!--<corners android:radius="50dp" />-->

<!--</shape>-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval"
    android:useLevel="false">

    <!-- 实心 -->
    <solid android:color="@color/colorOrigin" />
    <!-- 圆角 -->
    <corners android:radius="360dp" />
    <!-- 边距 -->
    <padding
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp" />

    <!-- 大小 -->
    <size
        android:width="15dp"
        android:height="15dp" />
</shape>